ipAddress           :   192.168.100.1
sendPort            :   0
receivePort         :   0
networkPort         :   2001
replyTimeout        :   30
retryCount          :   3
netTimeout          :   1000
pingTimeout[Sec]   	:   60
CmdSendLog          :   1
CmdRcvLog           :   1
CmdSendLog_Hex      :   0
CmdRcvLog_Hex       :   0
